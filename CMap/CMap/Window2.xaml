        xmlns:lm="clr-namespace:LibMap;assembly=LibMap"
        xmlns:chrome="clr-namespace:Microsoft.Windows.Shell;assembly=Microsoft.Windows.Shell"
        Title="Window2" Height="300" Width="300">
